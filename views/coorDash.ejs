<!DOCTYPE html>
<html lang="en">
 
  <head>
    <title>APOD Coordinator Dashboard</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="stylesheets/primarySiteTheme.css">
  </head>

  <body>
    <header>
    <h2>Coordinator Dashboard</h2>
    </header>

    <section>
    <nav>
        <a href="/profile" class="NavMenuButton"> My Profile</a>
        <a href="/application" class="NavMenuButton"> Application</a>
        <p class="NavMenuButton"> Onboarding </p>
        <p class="NavMenuButton"> Calendar </p>
        <a href="/volunteerpositions" class="NavMenuButton"> Volunteer </a>
        <a href="/logout" class="NavMenuButton">Log out</a>
        <p class = "NavMenuButton NavMenuSelection"> Dashboard </p>
    </nav>

    <article>
      <div id = "userProfile"
        <%- include partials/partialUserProfile.ejs %>
      </div>
    
    <h2>Training List</h2>
    <% trainings.forEach(function(training) { %>
      <li><%= training %> </li>
    <% }); %>

    <h2>Position List</h2>
    </article>
    </section>

    <footer>
    All Paws on Deck!
    </footer>

  </body>
</html>
